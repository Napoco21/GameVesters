<%= form_for :question, url: questions_path do |f| %>
  <p>
    <%= f.label :qid %><br>
    <%= f.text_field :qid %>
  </p>
 
  <p>
    <%= f.label :reward %><br>
    <%= f.text_field :reward %>
  </p>
<p>
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.label :ans1 %><br>
    <%= f.text_field :ans1 %>
  </p>
  <p>
    <%= f.label :ans2 %><br>
    <%= f.text_field :ans2 %>
  </p>
  <p>
    <%= f.label :ans3 %><br>
    <%= f.text_field :text %>
  </p>
<p>
    <%= f.label :ans4 %><br>
    <%= f.text_field :text %>
  </p>
  <p>
    <%= f.label :answer %><br>
    <%= f.text_field :text %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', questions_path %>